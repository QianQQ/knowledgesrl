<?xml version="1.0"?>
<!DOCTYPE dico_verbnet_rolemapping [
  <!ELEMENT dico_verbnet_rolemapping (lexie+)>
  <!ELEMENT lexie  (role+)>
  <!ELEMENT role   (#PCDATA)>
  <!ATTLIST lexie name  CDATA #REQUIRED>
  <!ATTLIST lexie vn    CDATA #REQUIRED>
  <!ATTLIST role  name  CDATA #REQUIRED>
  <!ATTLIST role  vn    CDATA #REQUIRED>
]>
<dico_verbnet_rolemapping ven='2014-11-05'>
    <lexie name='taper.1' sense='missinglemma' on='access.1' wn='access#1'>
    </lexie>
    <lexie name='acheminer.1' vn='send-11.1' sense='ok' alternations='more' roles='more'>
        <role name='Patient' vn='Theme' />
        <role name='Destination' vn='Destination' />
        <role name='Agent' vn='Agent' />
    </lexie>
